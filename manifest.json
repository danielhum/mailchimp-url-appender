{
  "name": "Mailchimp Link Appender",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Append a parameter to Mailchimp links",
  "homepage_url": "http://oneuphero.com",
  "content_scripts": [
    {
      "matches": [
        "https://us2.admin.mailchimp.com/campaigns/wizard/*"
      ],
      "js": [
        "js/jquery-2.0.3.min.js",
        "src/inject/inject.js"
      ]
    }
    ],
  "web_accessible_resources" : [
      "js/jquery-2.0.3.min.js"
  ]
}